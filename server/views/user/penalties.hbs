{{> navbarUser}}
<!-- Termina marcado de barra de navegación -->
<h1>Listado de Multas</h1>
<div class="row">
  {{#each penalties}}
  <div class="col s12 m6 l4">
    <div class="card horizontal">
      <div class="card-stacked">
        <div class="card-content">
          <span class="card-title">Multa</span>
          <p>Usuario: {{userId}}</p>
          <p>Monto: {{amount}}</p>
          <p>Razón: {{reason}}</p>
        </div>
      </div>
    </div>
  </div>
  {{else}}
  <p>No hay multas que listar.</p>
  {{/each}}
</div>