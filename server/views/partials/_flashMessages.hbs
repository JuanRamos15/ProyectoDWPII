{{#if successMessage}}
  <script>
    document.querySelector('script[src="/bundle.js"]').onload = ()=>{
      M.toast({
        html: "<i class='material-icons'>check_ricle</i> {{ successMessage }}", 
        classes: 'card-panel green accent-4 white-text',
        displayLength: 2000 })
    }
  </script>
{{/if}}
{{#if infoMessage}}
  <script>
    document.querySelector('script[src="/bundle.js"]').onload = ()=>{
      M.toast({html: "<i class='material-icons'>info</i> {{infoMessage}}", classes: 'info', displayLength: 2000 })
    }
  </script>
{{/if}}
{{#if errorMessage}}
  <script>
    document.querySelector('script[src="/bundle.js"]').onload = ()=>{
      M.toast({html: "<i class='material-icons'>error</i>{{errorMessage}}", classes: 'error', displayLength: 2000})
    }
  </script>
{{/if}}
{{#if passportError}}
  <script>
    document.querySelector('script[src="/bundle.js"]').onload = ()=>{
      M.toast({html: "<i class='material-icons'{{passportError}}", classes: 'error', displayLength: 2000})
    }
  </script>
{{/if}}