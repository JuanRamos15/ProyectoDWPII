{
  "name": "proyectodwpii",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./server/bin/www",
    "dev": "NODE_ENV=development DEBUG=dwpc2:server babel-watch ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "build": "npm run buildc && npm run builds",
    "builds": "npm run clean && babel ./server --out-dir dist --copy-files",
    "buildc": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "clean": "rm -rf ./dist",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-webpack-plugin": "^4.0.1",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.17",
    "@babel/node": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "babel-loader": "^9.1.3",
    "babel-watch": "^7.8.1",
    "core-js": "^3.32.2",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "mini-css-extract-plugin": "^2.7.6",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-hot-middleware": "^2.25.4"
  }
}
